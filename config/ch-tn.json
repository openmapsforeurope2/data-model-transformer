                        "electrified": { "eval": "'void_unk'" },
                        "speed_class": { "eval": "'void_unk'" },
                        "railway_use": { "eval": "'void_unk'" },
                        "access_restriction": {"eval": "'forbidden_legally' if data['ausser_betrieb'] == 'Wahr' else 'private' if data['betriebsbahn'] == 'Wahr' else 'public_access'"},
                        "railway_line_code": { "eval": "'void_unk'"},
                        "railway_line_name":  { "function": "ch_xx_name" },
                        "w_national_identifier": "uuid",
                        "xy_source": { "eval": "'nmca'"},
                        "z_source": { "eval": "'nmca'"},
                        "w_release": { "eval": "1"},
                        "w_scale": { "eval": "'10000'"}      
                    },
