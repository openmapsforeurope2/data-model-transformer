                        "geom": "geom"
                    },
                    "fetched_fields":[
                        "uuid",
                        "name",
                        "verkehrsmittel",
                        "anschlussgleis",
                        "objektart",
                        "stufe",
                        "ausser_betrieb",
                        "betriebsbahn"           
                    ]
