        "road":{
            "mock": false,
            "source_tables":{
                "route_numerotee_ou_nommee":{
                    "mock": false,
                    "where": "numero NOT LIKE 'E%' AND NOT gcms_detruit",
                    "mapping": {
                        "country": { "function": "fr_country_code" },
                        "road_name":  { "function": "fr_xx_name" },
