                        "track_gauge_category": {"eval": "'narrow' if data['objektart'] == 'Schmalspur' or data['objektart'] == 'Kleinbahn' else 'standard' if data['objektart'] == 'Normalspur' else 'void_unk'"},
                        "vertical_position": { "function": "ch_xx_vertical_position" },
                        "vertical_level": { "eval": "data['stufe'] if data['stufe'] is not None else '-32768'" },
                        "tent_network": { "eval": "'void_unk'" },
