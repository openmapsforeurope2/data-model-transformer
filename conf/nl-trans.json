{
    "country_code": "nl",
    "source_db":{
        "host":"10.128.35.60",
        "port":"5432",
        "name":"nl_national",
        "user":"postgres",
        "pwd":"postgres",
        "schema": "tn"
    },
    "source_srid": "28992",
    "target_db":{
        "host":"10.128.35.60",
        "port":"5432",
        "name":"ome2_hvlsp_release",
        "user":"ome2_user",
        "pwd":"ome2_user",
        "schema": "tn"
    },
    "target_country_field": "country",
    "target_tables": {
        "road_link":{
            "mock": false,
            "source_tables":{
                "top10nl_wegdeel_hartlijn": {
                    "mock": false,
                    "where": "",
                    "mapping": {
                        "country": { "eval": "'${country_code}'" },
                        "form_of_way": { "function": "nl_form_of_way" },
                        "functional_road_class": { "function": "nl_functional_road_class" },
                        "number_of_lanes": "aantalrijstroken",
                        "vertical_position": { "eval": "'underground' if data['hoogteniveau'] < 0 else 'suspended_or_elevated' if data['hoogteniveau'] > 0 else 'on_ground_surface'" },
                        "vertical_level": { "eval": "str(data['hoogteniveau'])" },
                        "trans_european_transport_network": { "eval": "'no_ten_t_network'" },
                        "geographical_name": "naam",
                        "road_surface_category": { "eval": "'unpaved' if data['verhardingstype'] == 'onverhard' else 'paved'" },           
                        "condition_of_facility": { "function": "nl_condition_of_facility" },
                        "w_national_identifier": { "eval": "data['namespace'] + data['lokaalid']" },
                        "national_road_code": { "function": "nl_national_road_code" },
                        "european_route_number":"ewegnummer"   
                    },
                    "geomapping":{
                        "geom": {
                            "geom":{
                                "transform": "ST_Force3D(${},-1000)"
                            }
                        }
                    },
                    "fetched_fields":[
                        "typeweg",
                        "hoofdverkeersgebruik",
                        "gescheidenrijbaan",
                        "fysiekvoorkomen",
                        "hoogteniveau",
                        "verhardingstype",
                        "status",
                        "namespace",
                        "lokaalid",
                        "awegnummer",
                        "nwegnummer"
                    ]
                }
            }
        }
    }
}