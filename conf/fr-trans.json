{
    "country_code": "fr",
    "source_db":{
        "host":"patafix3.ign.fr",
        "port":"5432",
        "name":"bduni_france",
        "user":"invite",
        "pwd":"28de#",
        "schema": "public"
    },
    "source_srid": "CASE gcms_territoire WHEN 'FXX' THEN 2154 WHEN 'GLP' THEN 4559 WHEN 'GUF' THEN 2972 WHEN 'H_T' THEN 4326 WHEN 'MTQ' THEN 4559 WHEN 'MYT' THEN 4471 WHEN 'REU' THEN 2975 WHEN 'SPM' THEN 4467 END",
    "target_db":{
        "host":"10.128.35.60",
        "port":"5432",
        "name":"ome2_hvlsp_v1",
        "user":"postgres",
        "pwd":"postgres",
        "schema": "tn"
    },
    "target_country_field": "country",
    "target_tables": {
        "road_link":{
            "mock": false,
            "source_tables":{
                "troncon_de_route": {
                    "mock": false,
                    "where": "NOT gcms_detruit AND nature != 'Bac ou liaison maritime' AND ST_intersects(geometrie,ST_GeomFromText('POLYGON((727735.9992572087 7029622.543797126,727544.2403708269 7068519.050552713,762523.9776143468 7068801.007743988,762959.2602054827 7029906.463932242,727735.9992572087 7029622.543797126))'))",
                    "mapping": {
                        "country": { "function": "fr_country_code" },
                        "form_of_way": { "function": "fr_form_of_way" },
                        "functional_road_class": { "function": "fr_functional_road_class" },
                        "number_of_lanes": { "eval": "data['nombre_de_voies'] if data['nombre_de_voies'] != 'Sans objet' else '0'" },
                        "vertical_position": { "function": "fr_vertical_position" },
                        "vertical_level": { "eval": "data['position_par_rapport_au_sol'] if data['position_par_rapport_au_sol'] != 'Gué ou radier' else '0'" },
                        "trans_european_transport_network": { "eval": "'no_ten_t_network'" },
                        "geographical_name": { "function": "fr_road_link_name" },
                        "road_surface_category": { "function": "fr_road_surface_category" },
                        "traffic_flow_direction": { "function": "fr_traffic_flow_direction" },
                        "access_restriction": { "function": "fr_access_restriction" },
                        "maximum_height": "restriction_de_hauteur",
                        "maximum_length": "restriction_de_longueur",
                        "maximum_width": "restriction_de_largeur",
                        "maximum_total_weight": "restriction_de_poids_total",
                        "maximum_single_axle_weight": "restriction_de_poids_par_essieu",
                        "condition_of_facility": { "function": "fr_condition_of_facility" },
                        "valid_from": "date_d_apparition",
                        "w_national_identifier": "cleabs",
                        "national_road_code": "cpx_numero",
                        "european_route_number":"cpx_numero_route_europeenne"    
                    },
                    "geomapping":{
                        "geom": "geometrie"
                    },
                    "fetched_fields":[
                        "nature",
                        "importance",
                        "sens_de_circulation",
                        "position_par_rapport_au_sol",
                        "nature_de_la_restriction",
                        "acces_vehicule_leger",
                        "periode_de_fermeture",
                        "etat_de_l_objet",
                        "gcms_territoire",
                        "nombre_de_voies",
                        "nom_1_gauche",
                        "nom_1_droite",
                        "date_d_apparition",
                        "cleabs",
                        "cpx_numero",
                        "cpx_numero_route_europeenne"
                    ]
                }
            }
        },
        "marker_post":{
            "mock": true,
            "source_tables":{
                "point_de_repere":{
                    "mock": false,
                    "where": "NOT gcms_detruit",
                    "mapping": {
                        "country": { "function": "fr_country_code" },
                        "link_to_road": { "eval": "'link_to_road'" },
                        "code": "identifiant_de_pr",
                        "distance": "abscisse"
                    },
                    "geomapping":{
                        "geom": "geometrie"
                    },
                    "fetched_fields":[
                        "gcms_territoire"
                    ]
                }
            }
        },
        "road_service":{
            "mock": true,
            "source_tables":{
                "equipement_de_transport":{
                    "mock": false,
                    "where": "NOT gcms_detruit AND nature IN ('Parking', 'Péage', 'Gare routière', 'Aire de repos ou de service')",
                    "mapping": {
                        "country": { "function": "fr_country_code" },
                        "type": { "function": "fr_road_service_type" },
                        "geographical_name": "toponyme"
                    },
                    "geomapping":{
                        "geom": {
                            "geometrie": {
                                "transform": "ST_PointOnSurface(${})"
                            }
                        },
                        "geom_area": "geometrie"
                    },
                    "fetched_fields":[
                        "nature",
                        "gcms_territoire"
                    ]
                }
            }
        },
        "railway_link":{
            "mock": true,
            "source_tables":{
                "troncon_de_voie_ferree":{
                    "mock": false,
                    "where": "NOT gcms_detruit AND nature != 'Sans objet'",
                    "mapping": {
                        "country": { "function": "fr_country_code" },
                        "type": { "function": "fr_railway_type_value" },
                        "number_of_tracks": "nombre_de_voies",
                        "vertical_position": { "function": "fr_vertical_position" },
                        "trans_european_transport_network": { "eval": "'no_ten_t_network'" },
                        "electrified": "electrifie"
                    },
                    "geomapping":{
                        "geom": "geometrie"
                    },
                    "fetched_fields":[
                        "nature",
                        "position_par_rapport_au_sol",
                        "gcms_territoire"
                    ]
                }
            }
        },
        "railway_station":{
            "mock": true,
            "source_tables":{
                "equipement_de_transport":{
                    "mock": false,
                    "where": "NOT gcms_detruit AND nature IN ('Arrêt voyageurs', 'Gare fret uniquement', 'Gare voyageurs et fret', 'Gare voyageurs uniquement', 'Station de métro', 'Station de tramway')",
                    "mapping": {
                        "country": { "function": "fr_country_code" },
                        "station_code": { "eval": "'station_code'" },
                        "railway_use": { "function": "fr_railway_use" }
                    },
                    "geomapping":{
                        "geom": {
                            "geometrie": {
                                "transform": "ST_PointOnSurface(${})"
                            }
                        },
                        "geom_area": "geometrie"
                    },
                    "fetched_fields":[
                        "nature",
                        "gcms_territoire"
                    ]
                }
            }
        }
    }
}