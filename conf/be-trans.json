{
    "country_code": "be",
    "source_db":{
        "host":"10.128.35.60",
        "port":"5432",
        "name":"be_national",
        "user":"ome2_user",
        "pwd":"ome2_user",
        "schema": "tn"
    },
    "source_srid": "4326",
    "target_db":{
        "host":"10.128.35.60",
        "port":"5432",
        "name":"ome2_hvlsp_v2",
        "user":"ome2_user",
        "pwd":"ome2_user",
        "schema": "tn"
    },
    "target_srid": "3035",
    "target_country_field": "country",
    "target_tables": {
        "road_link":{
            "mock": false,
            "source_tables":{
                "ro_roadsegment": {
                    "mock": false,
                    "where": "particpass = 4 or particpass = 999",
                    "mapping": {
                        "country": { "eval": "'${country_code}'" },
                        "form_of_way": { "function": "be_tn_roadlink_form_of_way" },
                        "functional_road_class": { "function": "be_tn_roadlink_functional_road_class" },
                        "number_of_lanes": { "eval": "data['lanesnb'] if data['lanesnb'] is not None else -32768" },
                        "road_surface_category": { "function": "be_tn_roadlink_road_surface_category" },
                        "condition_of_facility": { "function": "be_tn_roadlink_condition_of_facility" },
                        "national_road_code": { "eval": "data['natroadnb'] if data['natroadnb'] is not None else 'void'" },
                        "european_route_number": { "eval": "data['euroadnb'] if data['euroadnb'] is not None else 'void'" },
                        "w_national_identifier": "tgid"
                    },
                    "geomapping":{
                        "geom": "geom"
                    },
                    "fetched_fields":[
                        "modifdate",
                        "width",
                        "surftype",
                        "opstate",
                        "roadstatus",
                        "particpass",
                        "sepcarrway",
                        "natroadnb",
                        "euroadnb",
                        "tgid",
                        "lanesnb"
                    ]
                },
                "ro_dirtroadsegment":{
                    "mock": false,
                    "where": "",
                    "mapping": {
                        "country": { "eval": "'${country_code}'" },
                        "form_of_way": { "eval": "'tractor'" },
                        "road_surface_category": { "eval": "'unpaved'" },
                        "w_national_identifier": "tgid"
                    },
                    "geomapping":{
                        "geom": "geom"
                    },
                    "fetched_fields":[
                        "tgid"
                    ]
                }
            }
        },
        "marker_post":{
            "mock": true,
            "source_tables":{
                "ro_roadkilometremarker":{
                    "mock": false,
                    "where": "",
                    "mapping": {
                        "country": { "eval": "'${country_code}'" },
                        "distance": { "eval": "data['kilometre'] if data['kilometre'] is not None else -32768" }
                    },
                    "geomapping":{
                        "geom": "geom"
                    },
                    "fetched_fields":[
                        "kilometre"
                    ]
                }
            }
        }
    }
}